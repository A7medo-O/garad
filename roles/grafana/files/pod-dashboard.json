{
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "type": "graph",
      "title": "Pod CPU Usage",
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{image!=\"\",container!=\"POD\"}[5m])) by (pod, namespace)",
          "legendFormat": "{{namespace}}/{{pod}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Pod Memory Usage",
      "targets": [
        {
          "expr": "sum(container_memory_usage_bytes{image!=\"\",container!=\"POD\"}) by (pod, namespace)",
          "legendFormat": "{{namespace}}/{{pod}}",
          "refId": "B"
        }
      ]
    }
  ],
  "title": "Pod Metrics"
}
